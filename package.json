{
  "name": "examples",
  "version": "5.21.0",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf build dist lib",
    "tsc": "tsc",
    "ts-types": "tsc",
    "build": "npm run clean && tsc -p tsconfig.src.json",
    "build:dev": "npm run clean && tsc -p tsconfig.src.json",
    "build:prod": "npm run clean && tsc -p tsconfig.src.json",
    "build:watch": "tsc -p tsconfig.src.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint ./src --quiet --fix",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{ts,tsx,css,md,json}\"",
    "lint": "npm run lint:format && npm run lint:fix"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.27.0",
    "@effect/platform": "^0.88.2",
    "@effect/platform-node": "^0.91.0",
    "@types/lodash": "^4.17.20",
    "@types/ramda": "^0.30.2",
    "effect": "^3.16.16",
    "fastify": "^5.4.0",
    "lodash": "^4.17.21",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "ramda": "^0.31.3",
    "yaml": "^2.8.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/yaml": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "json-schema-to-ts": "^3.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.js"
    },
    "./core/*": {
      "types": "./build/core/*.d.ts",
      "import": "./build/core/*.js",
      "require": "./build/core/*.js"
    },
    "./examples/*": {
      "types": "./build/examples/*.d.ts",
      "import": "./build/examples/*.js",
      "require": "./build/examples/*.js"
    }
  },
  "files": [
    "build"
  ]
}
